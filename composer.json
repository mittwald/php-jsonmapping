{
  "name": "mittwald/jsonmapping",
  "description": "Library for mapping PHP objects to JSON structures",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Martin Helmich",
      "email": "m.helmich@mittwald.de"
    },
    {
      "name": "Sebastian Gieselmann",
      "email": "s.gieselmann@mittwald.de"
    }
  ],
  "require": {
    "php": ">=5.6"
  },
  "autoload": {
    "psr-4": {
      "Mw\\JsonMapping\\": "src"
    }
  },
  "autoload-dev": {
    "files": [
      "vendor/phpunit/phpunit/src/Framework/Assert/Functions.php"
    ],
    "psr-4": {
      "Mw\\JsonMapping\\Tests\\": "tests"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^5.2",
    "doctrine/orm": "^2.5"
  },
  "suggest": {
    "doctrine/orm": "Used for the Mw\\JsonMapping\\Doctrine\\CollectionMapping mapping"
  }
}
